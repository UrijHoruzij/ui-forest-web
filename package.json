{
	"name": "ui-forest",
	"version": "1.11.4",
	"dependencies": {
		"classnames": "^2.3.1",
		"normalize.css": "^8.0.1",
		"prop-types": "^15.7.2",
		"uuid": "^8.3.2"
	},
	"peerDependencies": {
		"react": ">=17",
		"react-dom": ">=17"
	},
	"scripts": {
		"test": "react-scripts test",
		"build": "npm run rm && npm run babel && npm run build-theme && npm run copy-css && npm run copy-themes && npm run copy-fonts",
		"rm": "rimraf ./dist",
		"babel": "cross-env NODE_ENV=production babel src/components --out-dir dist",
		"copy-fonts": "copyfiles --flat src/components/**/*.woff2 dist/Global/Fonts",
		"copy-css": "copyfiles --up 2 src/components/**/*.css dist",
		"copy-themes": "copyfiles --flat src/components/ThemeProvider/**/*.css dist/ThemeProvider/themes",
		"storybook": "start-storybook -p 6006 -s public --no-manager-cache",
		"build-storybook": "build-storybook -o docs-build",
		"build-theme": "node ./src/themes/generator.mjs",
		"prepare": "husky install"
	},
	"author": "Urij Horuzij",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"files": [
		"dist",
		"README.md"
	],
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.18.9",
		"@babel/core": "^7.18.9",
		"@babel/preset-env": "^7.18.9",
		"@babel/preset-react": "^7.18.6",
		"@commitlint/cli": "^17.0.0",
		"@commitlint/config-conventional": "^17.0.0",
		"@semantic-release/changelog": "^6.0.2",
		"@semantic-release/git": "^10.0.1",
		"@storybook/addon-actions": "^6.3.7",
		"@storybook/addon-essentials": "^6.3.7",
		"@storybook/addon-links": "^6.3.7",
		"@storybook/node-logger": "^6.3.7",
		"@storybook/preset-create-react-app": "^3.2.0",
		"@storybook/react": "^6.3.7",
		"@storybook/theming": "^6.3.6",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"generator-css-variables": "^1.0.1",
		"husky": "^8.0.1",
		"prettier": "^2.5.1",
		"react": "^17.0.0",
		"react-dom": "^17.0.0",
		"react-scripts": "4.0.3",
		"react-test-renderer": "^17.0.2",
		"rimraf": "^3.0.2",
		"storybook-addon-themes": "^6.1.0"
	}
}
